#!/bin/bash
USER=$1
source /var/cld/bin/include/cldfuncs
ALLOWED() {
for GROUP in $(cat /var/cld/access/users/${USER}/groups 2>/dev/null)
do
cut -d _ -f -4  /var/cld/access/groups/${GROUP}/clouds 2>/dev/null | grep -vh "^$\|^#"
done
cut -d _ -f -4  /var/cld/access/users/${USER}/clouds 2>/dev/null | grep -vh "^$\|^#" 2>/dev/null
echo " "
}
VM_LIST | grep -v "$(ALLOWED)"

